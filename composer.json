{
    "name": "drupal/legacy-project",
    "description": "Project template for Drupal 9 projects with composer following drupal/drupal layout",
    "type": "project",
    "license": "GPL-2.0-or-later",
    "homepage": "https://www.drupal.org/project/drupal",
    "support": {
        "docs": "https://www.drupal.org/docs/user_guide/en/index.html",
        "chat": "https://www.drupal.org/node/314178"
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    ],
    "require": {
        "composer/installers": "^1.9",
        "cweagans/composer-patches": "^1.7",
        "drupal/addanother": "^1.1",
        "drupal/admin_toolbar": "^3.1",
        "drupal/anchor_link": "^2.5",
        "drupal/antibot": "^1.5",
        "drupal/back_to_top": "2.0",
        "drupal/backup_migrate": "^5.0",
        "drupal/block_class": "^1.3",
        "drupal/block_field": "^1.0@RC",
        "drupal/chosen": "^3.0",
        "drupal/components": "^2.4",
        "drupal/core-composer-scaffold": "^9.3",
        "drupal/core-project-message": "^9.3",
        "drupal/core-recommended": "^9.3",
        "drupal/core-vendor-hardening": "^9.3",
        "drupal/current_year": "^1.0",
        "drupal/devel": "^4.1",
        "drupal/drupal_slider": "^2.0",
        "drupal/editor_advanced_link": "2.0",
        "drupal/eva": "2.1",
        "drupal/examples": "3.0",
        "drupal/externalauth": "^2.0",
        "drupal/flexslider": "^2.0",
        "drupal/fontawesome": "2.19",
        "drupal/google_tag": "1.5",
        "drupal/graphql": "4.3",
        "drupal/group": "1.4",
        "drupal/group_content_menu": "1.1",
        "drupal/gutenberg": "^2.4",
        "drupal/honeypot": "^2.0",
        "drupal/imagemagick": "^3.3",
        "drupal/jsonapi_extras": "3.21",
        "drupal/jsonapi_hypermedia": "^1.7",
        "drupal/jsonapi_menu_items": "1.2",
        "drupal/jsonapi_views": "1.0",
        "drupal/link_target": "^1.3",
        "drupal/masonry": "^1.2",
        "drupal/masonry_views": "^1.1",
        "drupal/metatag": "^1.19",
        "drupal/miniorange_saml": "^2.25",
        "drupal/module_filter": "^3.2",
        "drupal/next": "^1.1",
        "drupal/node_revision_delete": "^1.0@RC",
        "drupal/nodeaccess": "1.1",
        "drupal/override_node_options": "^2.6",
        "drupal/panelizer": "4.4",
        "drupal/panels": "^4.6",
        "drupal/paragraphs": "^1.12",
        "drupal/pathauto": "^1.8",
        "drupal/prlp": "^1.7",
        "drupal/recaptcha": "3.0",
        "drupal/redirect": "^1.7",
        "drupal/responsive_menu": "4.4",
        "drupal/restui": "1.20",
        "drupal/search_api": "^1.25",
        "drupal/search_api_autocomplete": "^1.6",
        "drupal/search_autocomplete": "^2.0",
        "drupal/slick": "2.6",
        "drupal/slick_views": "2.6",
        "drupal/stage_file_proxy": "^1.2",
        "drupal/subrequests": "^3.0",
        "drupal/super_login": "^1.7",
        "drupal/superfish": "^1.4",
        "drupal/svg_image": "^1.15",
        "drupal/svg_image_field": "2.0.0-beta1",
        "drupal/tb_megamenu": "1.6",
        "drupal/timefield": "1.0",
        "drupal/twig_debugger": "1.1",
        "drupal/tzfield": "1.5",
        "drupal/video_embed_field": "2.4",
        "drupal/views_layout": "1.2",
        "drupal/views_slideshow": "^4.8",
        "drupal/views_taxonomy_parent_id_from_term": "1.5",
        "drupal/views_templates": "1.1",
        "drupal/we_megamenu": "^1.13",
        "drupal/webform": "^6.1",
        "drupal/xmlsitemap": "^1.2",
        "drush/drush": "^11.0",
        "wikimedia/composer-merge-plugin": "^2.0"
    },
    "conflict": {
        "drupal/drupal": "*"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "composer/installers": true,
            "drupal/core-composer-scaffold": true,
            "drupal/core-project-message": true,
            "wikimedia/composer-merge-plugin": true,
            "cweagans/composer-patches": true,
            "drupal/core-vendor-hardening": true
        }
    },
    "extra": {
        "drupal-scaffold": {
            "locations": {
                "web-root": "./"
            }
        },
        "merge-plugin": {
            "include": [
                "modules/contrib/webform/composer.libraries.json"
            ]
        },
        "patches": {
            "drupal/search_autocomplete": {
                "value.autocomplete is not a function - https://www.drupal.org/i/3220334": "https://www.drupal.org/files/issues/2021-06-24/value-autocomplete-not-function-3220334-3.patch"
            }
        },
        "installer-paths": {
            "core": [
                "type:drupal-core"
            ],
            "libraries/{$name}": [
                "type:drupal-library"
            ],
            "modules/contrib/{$name}": [
                "type:drupal-module"
            ],
            "profiles/contrib/{$name}": [
                "type:drupal-profile"
            ],
            "themes/contrib/{$name}": [
                "type:drupal-theme"
            ],
            "drush/Commands/contrib/{$name}": [
                "type:drupal-drush"
            ],
            "modules/custom/{$name}": [
                "type:drupal-custom-module"
            ],
            "profiles/custom/{$name}": [
                "type:drupal-custom-profile"
            ],
            "themes/custom/{$name}": [
                "type:drupal-custom-theme"
            ]
        },
        "drupal-core-project-message": {
            "include-keys": [
                "homepage",
                "support"
            ],
            "post-create-project-cmd-message": [
                "<bg=blue;fg=white>                                                         </>",
                "<bg=blue;fg=white>  Congratulations, youâ€™ve installed the Drupal codebase  </>",
                "<bg=blue;fg=white>  from the drupal/legacy-project template!               </>",
                "<bg=blue;fg=white>                                                         </>",
                "",
                "<bg=yellow;fg=black>Next steps</>:",
                "  * Install the site: https://www.drupal.org/docs/8/install",
                "  * Read the user guide: https://www.drupal.org/docs/user_guide/en/index.html",
                "  * Get support: https://www.drupal.org/support",
                "  * Get involved with the Drupal community:",
                "      https://www.drupal.org/getting-involved",
                "  * Remove the plugin that prints this message:",
                "      composer remove drupal/core-project-message"
            ]
        }
    }
}
